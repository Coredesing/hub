{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@base-components/*": [
                "src/components/Base/*"
            ],
            "@layout-components/*": [
                "src/components/Layout/*"
            ],
            "@contants/*": [
                "src/contants/*"
            ],
            "@abi/*": [
                "src/abi/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@utils/*": ["src/utils/*"],
            "@pages/*": ["src/pages/*"],
            "@styles/*": ["src/styles/*"],
            "@store/*": ["src/store/*"],
            "@app-constants": ["src/constants"],
            "@request/*": ["src/request/*"],
            "@app-types": ["src/types"],
            "@app-types/*": ["src/types/*"]
        }
    }
}